{
    "parser": "babel-eslint",
    "extends": "react",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "indent": "off",
        "padded-blocks": "off",
        "space-before-function-paren": "off",
        "no-trailing-spaces": "off"
    }
}